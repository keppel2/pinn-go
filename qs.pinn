A [fill]int = 1, 10, 3, 2, 5, 20, -4;
func quickSort(p int, r int)
{
	q int;
	if p < r {
		q = partition(p, r);
		quickSort(p, q - 1);
		quickSort(q + 1, r);
	}
}

func partition(p int, r int) int
{
	x int = A[r];
	i int = p - 1;
	j int;
	temp int;
	fo j = p; j <= r - 1; j++ {
		if A[j] <= x {
			i++;
			temp = A[i];
			A[i] = A[j];
			A[j] = temp;
		}
	}
	temp = A[i+1];
	A[i+1] = A[r];
	A[r] = temp;
	return i + 1;
}

func main()
{
	print (A);
	quickSort(0, len (A) - 1);
	print (A);
}
